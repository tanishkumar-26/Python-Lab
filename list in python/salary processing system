salaries = [25000, 60000, 45000, 80000, 52000]

# Remove below minimum wage (assume 20000)
salaries = [s for s in salaries if s >= 20000]

# Add 5% bonus if salary > 50000
for i in range(len(salaries)):
    if salaries[i] > 50000:
        salaries[i] *= 1.05

# Sort descending
salaries.sort(reverse=True)

print("Top 3 Highest Salaries:", salaries[:3])