scores = [25, 34, 50, 60, 32, 80, 40]

# Remove lowest 2 scores
scores.sort()
scores = scores[2:]

# Add grace marks
for i in range(len(scores)):
    if 30 <= scores[i] <= 35:
        scores[i] += 5

# Count passed students (>= 40)
passed = len([s for s in scores if s >= 40])

print("Updated Scores:", scores)
print("Number of Passed Students:", passed)